{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/*.{css,js,png,jpg,jpeg,gif,svg,ico,webp,txt,map}"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  }
}